{
  "presets": [["env", { "modules": false }], ["stage-2"], ["react"]],
  "plugins": [
    "transform-export-extensions",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}
