{
  "env": {
    "test": {
      "presets": ["env", "stage-2", "react"],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],   
        "transform-export-extensions",
        [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
          }
        ]
      ]
    },
    "cjs": {
      "presets": ["env", "stage-2", "react"],
      "plugins": [
        "transform-export-extensions",
        [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
          }
        ]
      ]
    },
    "umd": {
      "presets": [["env", { "modules": false }], "stage-2", "react"],
      "plugins": ["transform-export-extensions", "external-helpers"]
    },
    "development": {
      "presets": [["env", { "modules": false }], ["stage-2"], ["react"]],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        "transform-export-extensions",
        [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
          }
        ]
      ]
    }
  }
}
